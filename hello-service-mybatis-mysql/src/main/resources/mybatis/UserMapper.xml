<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdhs.oms.helloservice.dao.UserMapper">


    <select id="findUser" parameterType="integer"
            resultType="com.zdhs.oms.helloserviceapi.UserEntity">
        select *
        from user where age = #{age}
    </select>

    <insert id="saveUser" parameterType="com.zdhs.oms.helloserviceapi.UserEntity">
        insert into user(age)
        values(#{user.age})
    </insert>

    <delete id="deleteUser" parameterType="integer">
        delete from user where age = #{age}
    </delete>
</mapper>